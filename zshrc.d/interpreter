if type rbenv &> /dev/null; then eval "$(rbenv init -)"; fi
if type pyenv &> /dev/null; then eval "$(pyenv init -)"; fi
if type plenv &> /dev/null; then eval "$(plenv init -)"; fi
if type exenv &> /dev/null; then eval "$(exenv init -)"; fi
if type ndenv &> /dev/null; then eval "$(ndenv init -)"; fi
