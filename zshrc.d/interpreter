if which -s rbenv  > /dev/null; then eval "$(rbenv init -)"; fi
if which -s pyenv  > /dev/null; then eval "$(pyenv init -)"; fi
if which -s plenv  > /dev/null; then eval "$(plenv init -)"; fi
if which -s exenv  > /dev/null; then eval "$(exenv init -)"; fi

source $(brew --prefix nvm)/nvm.sh
if [ -d "$HOME/.nvm" ]; then
    export NVM_DIR=~/.nvm
    nvm use default > /dev/null
    source <(npm completion)
fi
